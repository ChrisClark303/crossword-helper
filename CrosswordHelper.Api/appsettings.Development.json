{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.AzureLogAnalytics" ],
    "MinimumLevel": "Information",
    "Override": {
      "System": "Information",
      "Microsoft": "Information",
      "Microsoft.AspNetCore.Authentication": "Information",
      "Microsoft.AspNetCore.SignalR": "Debug",
      "Microsoft.AspNetCore.Http.Connections": "Debug"
    },
    "WriteTo": [
      {
        "Name": "AzureAnalytics",
        "Args": {
          "logName": "{LogName}",
          "authenticationId": "{primaryKey}",
          "workspaceId": "{workspaceId}"
        }
      },
      {
        "Name": "Console"
      }
    ],
    "Enrich": [ "FromLogContext" ]
  },
  "ConnectionStrings": {
    "crosswordHelper": "User Id=user;Password=pass;Host=localhost;Port=5432;Database=crossword_helper"
  }
}
